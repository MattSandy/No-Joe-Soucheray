{
  "manifest_version": 2,

  "name": "No Joe Soucheray",
  "description": "This extension will hide Joe Soucheray related elements.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage", "activeTab","tabs"
  ],
  "content_scripts": [{
      "matches": ["http://www.twincities.com/*","https://www.twincities.com/*"],
      "js": ["jquery.min.js","remove.js","options.js"]
  }]
}